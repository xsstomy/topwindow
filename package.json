{
    "name": "topwindow-website",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "type-check": "tsc --noEmit",
        "build:worker": "npx @opennextjs/cloudflare build",
        "preview:worker": "npx @opennextjs/cloudflare preview",
        "pre": "npm run build:worker && npm run preview:worker",
        "deploy": "npm run build:worker && wrangler deploy",
        "dev:workers": "wrangler dev",
        "clean": "rm -rf .next .open-next node_modules/.cache"
    },
    "dependencies": {
        "@supabase/auth-helpers-nextjs": "^0.10.0",
        "@supabase/supabase-js": "^2.55.0",
        "framer-motion": "^10.16.0",
        "lucide-react": "^0.294.0",
        "next": "^14.0.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "resend": "^6.0.1",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20250829.0",
        "@opennextjs/cloudflare": "^1.6.5",
        "@types/node": "^20.0.0",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@types/uuid": "^10.0.0",
        "autoprefixer": "^10.0.0",
        "esbuild": "^0.25.9",
        "eslint": "^8.0.0",
        "eslint-config-next": "^14.0.0",
        "postcss": "^8.0.0",
        "tailwindcss": "^3.3.0",
        "typescript": "^5.0.0"
    }
}
